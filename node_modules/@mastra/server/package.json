{
  "name": "@mastra/server",
  "version": "0.21.1",
  "description": "",
  "type": "module",
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./handlers": {
      "import": {
        "types": "./dist/server/handlers.d.ts",
        "default": "./dist/server/handlers.js"
      },
      "require": {
        "types": "./dist/server/handlers.d.ts",
        "default": "./dist/server/handlers.cjs"
      }
    },
    "./handlers/*": {
      "import": {
        "types": "./dist/server/handlers/*.d.ts",
        "default": "./dist/server/handlers/*.js"
      },
      "require": {
        "types": "./dist/server/handlers/*.d.ts",
        "default": "./dist/server/handlers/*.cjs"
      }
    },
    "./a2a/store": {
      "import": {
        "types": "./dist/server/a2a/store.d.ts",
        "default": "./dist/server/a2a/store.js"
      },
      "require": {
        "types": "./dist/server/a2a/store.d.ts",
        "default": "./dist/server/a2a/store.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "peerDependencies": {
    "@mastra/core": ">=0.21.0-0 <0.22.0-0",
    "zod": "^3.25.0 || ^4.0.0"
  },
  "devDependencies": {
    "@ai-sdk/openai": "^1.3.24",
    "@ai-sdk/openai-v5": "npm:@ai-sdk/openai@2.0.42",
    "@microsoft/api-extractor": "^7.52.8",
    "@types/node": "^20.19.0",
    "eslint": "^9.36.0",
    "superjson": "^2.2.2",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "zod": "^3.25.76",
    "@internal/lint": "0.0.50",
    "@mastra/core": "0.21.1",
    "@internal/storage-test-utils": "0.0.46",
    "@mastra/agent-builder": "0.2.0",
    "@internal/types-builder": "0.0.25"
  },
  "homepage": "https://mastra.ai",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mastra-ai/mastra.git",
    "directory": "packages/server"
  },
  "bugs": {
    "url": "https://github.com/mastra-ai/mastra/issues"
  },
  "scripts": {
    "build": "tsup --silent --config tsup.config.ts",
    "build:watch": "pnpm build --watch",
    "pull:openapispec": "node src/server/openapi.script.js",
    "test": "vitest run",
    "lint": "eslint ."
  }
}